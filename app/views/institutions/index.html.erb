<h1>Listing institutions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Phones</th>
    <th>Contact</th>
    <th>Institution type</th>
    <th>User</th>
    <th>Quantity team</th>
  </tr>

<% @institutions.each do |institution| %>
  <tr>
    <td><%=h institution.name %></td>
    <td><%=h institution.address %></td>
    <td><%=h institution.phones %></td>
    <td><%=h institution.contact %></td>
    <td><%=h institution.institution_type_id %></td>
    <td><%=h institution.user_id %></td>
    <td><%=h institution.quantity_team %></td>
    <td><%= link_to 'Show', institution %></td>
    <td><%= link_to 'Edit', edit_institution_path(institution) %></td>
    <td><%= link_to 'Destroy', institution, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New institution', new_institution_path %>