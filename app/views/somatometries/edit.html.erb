<h1>Editing somatometry</h1>

<% form_for(@somatometry) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.label :talla %><br />
    <%= f.text_field :talla %>
  </p>
  <p>
    <%= f.label :sistolica %><br />
    <%= f.text_field :sistolica %>
  </p>
  <p>
    <%= f.label :diastolica %><br />
    <%= f.text_field :diastolica %>
  </p>
  <p>
    <%= f.label :peso %><br />
    <%= f.text_field :peso %>
  </p>
  <p>
    <%= f.label :temperatura %><br />
    <%= f.text_field :temperatura %>
  </p>
  <p>
    <%= f.label :fc %><br />
    <%= f.text_field :fc %>
  </p>
  <p>
    <%= f.label :fr %><br />
    <%= f.text_field :fr %>
  </p>
  <p>
    <%= f.label :patient_id %><br />
    <%= f.text_field :patient_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :dependency_id %><br />
    <%= f.text_field :dependency_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @somatometry %> |
